<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Sahool Field App</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>sahool_field_app</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<!-- Global settings -->
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<!-- Pinned domains configuration -->
		<key>NSPinnedDomains</key>
		<dict>
			<!-- Production API domain: api.sahool.io -->
			<key>api.sahool.io</key>
			<dict>
				<key>NSIncludesSubdomains</key>
				<true/>
				<key>NSPinnedLeafIdentities</key>
				<array>
					<dict>
						<!-- Primary SPKI hash - REPLACE WITH ACTUAL VALUE -->
						<!-- Get using: openssl s_client -connect api.sahool.io:443 | openssl x509 -pubkey -noout | openssl pkey -pubin -outform der | openssl dgst -sha256 -binary | openssl enc -base64 -->
						<key>SPKI-SHA256-BASE64</key>
						<string>AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA=</string>
					</dict>
					<dict>
						<!-- Backup SPKI hash for rotation - REPLACE WITH ACTUAL VALUE -->
						<key>SPKI-SHA256-BASE64</key>
						<string>BBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBB=</string>
					</dict>
				</array>
			</dict>
			<!-- Production API domain: api.sahool.app -->
			<key>api.sahool.app</key>
			<dict>
				<key>NSIncludesSubdomains</key>
				<true/>
				<key>NSPinnedLeafIdentities</key>
				<array>
					<dict>
						<!-- Primary SPKI hash - REPLACE WITH ACTUAL VALUE -->
						<key>SPKI-SHA256-BASE64</key>
						<string>AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA=</string>
					</dict>
					<dict>
						<!-- Backup SPKI hash for rotation - REPLACE WITH ACTUAL VALUE -->
						<key>SPKI-SHA256-BASE64</key>
						<string>BBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBB=</string>
					</dict>
				</array>
			</dict>
			<!-- Staging API domain: api-staging.sahool.app -->
			<key>api-staging.sahool.app</key>
			<dict>
				<key>NSIncludesSubdomains</key>
				<true/>
				<key>NSPinnedLeafIdentities</key>
				<array>
					<dict>
						<!-- Staging SPKI hash - REPLACE WITH ACTUAL VALUE -->
						<key>SPKI-SHA256-BASE64</key>
						<string>EEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE=</string>
					</dict>
				</array>
			</dict>
			<!-- WebSocket production: ws.sahool.app -->
			<key>ws.sahool.app</key>
			<dict>
				<key>NSIncludesSubdomains</key>
				<true/>
				<key>NSPinnedLeafIdentities</key>
				<array>
					<dict>
						<!-- WebSocket SPKI hash - REPLACE WITH ACTUAL VALUE -->
						<key>SPKI-SHA256-BASE64</key>
						<string>CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC=</string>
					</dict>
				</array>
			</dict>
			<!-- WebSocket staging: ws-staging.sahool.app -->
			<key>ws-staging.sahool.app</key>
			<dict>
				<key>NSIncludesSubdomains</key>
				<true/>
				<key>NSPinnedLeafIdentities</key>
				<array>
					<dict>
						<!-- Staging WebSocket SPKI hash - REPLACE WITH ACTUAL VALUE -->
						<key>SPKI-SHA256-BASE64</key>
						<string>FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF=</string>
					</dict>
				</array>
			</dict>
		</dict>
		<!-- Exception for localhost in development -->
		<key>NSExceptionDomains</key>
		<dict>
			<key>localhost</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
			<key>10.0.2.2</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
		</dict>
	</dict>
</dict>
</plist>
