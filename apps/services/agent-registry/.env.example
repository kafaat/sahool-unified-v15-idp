# SAHOOL Agent Registry Service - Environment Configuration
# ==========================================================

# Service Configuration
SERVICE_NAME=agent-registry
SERVICE_PORT=8080
LOG_LEVEL=INFO
ENVIRONMENT=production

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_PREFIX=sahool:registry:

# Registry Configuration
HEALTH_CHECK_INTERVAL_SECONDS=60
HEALTH_CHECK_TIMEOUT_SECONDS=5
ENABLE_AUTO_DISCOVERY=false
AGENT_TTL_SECONDS=3600

# Security
REQUIRE_API_KEY=true
API_KEY=your-secret-api-key-here

# CORS
CORS_ORIGINS=https://sahool.app,https://admin.sahool.app,https://api.sahool.app,http://localhost:3000,http://localhost:5173
