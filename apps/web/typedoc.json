{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "./docs",
  "name": "SAHOOL Web App - API Documentation",
  "readme": "./README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "excludeNotDocumented": false,
  "exclude": [
    "**/node_modules/**",
    "**/__tests__/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx"
  ],
  "theme": "default",
  "plugin": [],
  "categorizeByGroup": true,
  "categoryOrder": [
    "Features",
    "Hooks",
    "Components",
    "API",
    "Types",
    "Utils",
    "*"
  ],
  "navigationLinks": {
    "GitHub": "https://github.com/kafaat/sahool-unified-v15-idp",
    "SAHOOL": "https://sahool.com"
  },
  "searchInComments": true,
  "cleanOutputDir": true,
  "gitRevision": "main",
  "hideGenerator": false,
  "sort": ["source-order", "alphabetical"],
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}
