{
  "name": "@sahool/shared-types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for SAHOOL platform",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth.js",
      "require": "./dist/auth.js",
      "types": "./dist/auth.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "require": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./express": {
      "import": "./dist/express.js",
      "require": "./dist/express.js",
      "types": "./dist/express.d.ts"
    },
    "./websocket": {
      "import": "./dist/websocket.js",
      "require": "./dist/websocket.js",
      "types": "./dist/websocket.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "sahool",
    "types",
    "typescript"
  ],
  "author": "SAHOOL",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  }
}
